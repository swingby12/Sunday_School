<h1>Listing accesses</h1>

<table>
  <tr>
    <th>Type</th>
    <th>Write</th>
    <th>Create</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @accesses.each do |access| %>
  <tr>
    <td><%= access.type %></td>
    <td><%= access.write %></td>
    <td><%= access.create %></td>
    <td><%= access.user_id %></td>
    <td><%= link_to 'Show', access %></td>
    <td><%= link_to 'Edit', edit_access_path(access) %></td>
    <td><%= link_to 'Destroy', access, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Access', new_access_path %>
